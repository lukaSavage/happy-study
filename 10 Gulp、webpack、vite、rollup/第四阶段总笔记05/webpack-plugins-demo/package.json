{
  "name": "webpack-plugins-demo",
  "version": "1.0.0",
  "description": "演示自定义 Webpack 插件的实现",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack-demo.config.js --mode=production",
    "dev": "webpack --config webpack-demo.config.js --mode=development",
    "analyze": "webpack --config webpack-demo.config.js --mode=production",
    "clean": "node -e \"require('./clean-output-plugin').prototype.removeDirectory('dist')\"",
    "serve": "webpack serve --config webpack-demo.config.js --mode=development"
  },
  "devDependencies": {
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0"
  },
  "keywords": [
    "webpack",
    "plugin",
    "custom",
    "demo"
  ],
  "author": "Your Name",
  "license": "MIT"
}